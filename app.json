{
  "name": "my-telegram-bot",
  "description": "A Telegram bot to download videos",
  "repository": "https://github.com/username/repository",
  "env": {
    "API_ID": {
      "description": "API ID for Telegram",
      "value": "your_api_id"
    },
    "API_HASH": {
      "description": "API Hash for Telegram",
      "value": "your_api_hash"
    },
    "BOT_TOKEN": {
      "description": "Bot token for Telegram",
      "value": "your_bot_token"
    },
    "DEVELOPER_ID": {
      "description": "Developer's Telegram ID",
      "value": "your_developer_id"
    },
    "REQUIRED_CHANNELS": {
      "description": "List of required channels",
      "value": "channel_username1,channel_username2"
    }
  }
}
